<body>

  <!-- NAVBAR + BACK TO TOP-->
  <%- include('../partials/navbar', {sessione: sessione}); %>

  <% if (message) { %>
    <div style="text-align: center" class="alert alert-<%=message.type%>">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong><%= message.intro %></strong> <%= message.message %>
    </div>
  <% } %>

  <div class="container py-4">

    <div class="row">
      <div class="col text-center">
        <h2 class="display-3 title">About Us</h2>
        <p>This website has been designed by Matteo Panzironi and Valentina Belli as the final project of the course "Linguaggi e tecnologie per il web AA 2021/2022" held by professor R. Rosati.</p>
      </div>
    </div>

    <div class="row">
      <div class="col justify-content-between text-center">
  
      <div>
        <img src="/images/1653054762083.jpg" class="round-pic ratio-1x1" loading="lazy">
      </div>
        <h1 class="name-text">Matteo Panzironi</h1>
        <h2 class="username-text"><a class="text-black" href="user/panzomat">@panzomat</a></h2>
        
    </div>

    <div class="col justify-content-between text-center">
        <div>
          <img src="/images/1653927980834.jpg" class="round-pic ratio-1x1" loading="lazy">
        </div>
          <h1 class="name-text">Valentina Belli</h1>
          <h2 class="username-text"><a class="text-black" href="user/val7e">@val7e</a></h2>
    </div>
    </div>
    

